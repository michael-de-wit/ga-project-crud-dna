<form action="/<%= dataPoint._id %>?_method=PUT" method="POST">
    <label>Template nucleotide:</label>
    <input name="templateNucleotide" type="text" value="<%= dataPoint.templateNucleotide %>"/>
    <label>Coding nucleotide:</label>
    <input name="codingNucleotide" type="text" value="<%= dataPoint.codingNucleotide %>"/>
    <button type="submit">Update</button>
</form>
<form action="/<%= dataPoint._id %>?_method=DELETE" method="POST">
    <button type="submit">Delete</button>
</form>