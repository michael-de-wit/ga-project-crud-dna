<div id="dataArray">
    <p> [
    <% for (const [index, dataPoint] of dataPoints.entries()) { %> <!-- Loop through dataPoints to clean up the object format -->
        <% if(index + 1 != dataPoints.length) { %> <!-- Add a comma after each object unless it's the last object -->
           {"nucleotidePosition": <%= dataPoint.nucleotidePosition %>, "templateNucleotide": "<%= dataPoint.templateNucleotide %>", "codingNucleotide": "<%= dataPoint.codingNucleotide %>", "nucleotideMatch": <%= dataPoint.nucleotideMatch %>, "dateAdded": "<%= dataPoint.dateAdded %>" },
        <% } else { %> <!-- This should only apply to the last object -- excludes the comma afterwards -->
           {"nucleotidePosition": <%= dataPoint.nucleotidePosition %>, "templateNucleotide": "<%= dataPoint.templateNucleotide %>", "codingNucleotide": "<%= dataPoint.codingNucleotide %>", "nucleotideMatch": <%= dataPoint.nucleotideMatch %>, "dateAdded": "<%= dataPoint.dateAdded %>" }
        <% } %>
    <% } %>
        ]
    </p>
</div>